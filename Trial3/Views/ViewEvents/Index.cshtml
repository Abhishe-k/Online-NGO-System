@model IEnumerable<Trial3.Models.Event>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br /><br /><br />
<table class="table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EventTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Venue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
 
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EventTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Venue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        
        <td>
             @Html.ActionLink("Details", "Details", new { id = item.EventId }) |
             @Html.ActionLink("Register", "getQuantity", new { event_id = item.EventId })
        </td>
    </tr>
}

</table>
